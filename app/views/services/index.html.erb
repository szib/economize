<div class="ui container">
  <div class="ui three column cards">
    <% @services.each do |service| %>
      <% content_for :card_bottom, flush: true do  %>
        <div class="ui bottom attached button">
          <i class="add icon"></i>
          <%= link_to 'More', service %>
        </div>
      <% end %>
      <%= render 'card', service: service %>
    <% end %>
  </div>
</div>
